Names = classNames('avatar', className);\n\n  const sizes = {\n    small: '50px',\n    medium: '80px',\n    large: '120px'\n  }\n\n  const aStyle = {\n    borderRadius: '100%',\n    display: 'inline-block',\n    height: sizes[size],\n    width: sizes[size]\n  }; \n\n  const imgStyle = {\n    borderRadius: '100%',\n    display: 'block',\n    height: sizes[size],\n    width: sizes[size]\n  }; \n\n  const avatarUrl = Users.avatar.getUrl(user);\n\n  const img = <img alt={Users.getDisplayName(user)} style={imgStyle} className={classNames('shadow-lg', avatarClassNames)} src={avatarUrl} title={user.username} />;\n  const initials = <span className={avatarClassNames}><span>{Users.avatar.getInitials(user)}</span></span>;\n\n  const avatar = avatarUrl ? img : initials;\n\n  return link ? <Link style={aStyle} className={avatarClassNames} to={Users.getProfileUrl(user)}>{avatar}</Link> : avatar;\n\n};\n\nAvatar.propTypes = {\n  user: PropTypes.object.isRequired,\n  size: PropTypes.string,\n  link: PropTypes.bool\n}\n\nAvatar.defaultProps = {\n  size: 'medium',\n  link: true\n}\n\nGrudr.registerComponent('Avatar', Avatar);\n","/Users/vipin.lahoti/Documents/meteor/Firebase/components/common/Button.jsx",["138","139","140"],"import Grudr from '@grudr';\nimport React, { Component } from 'react';\nimport Link from 'next/link';\nimport Button from 'react-bootstrap/Button';\n\nclass GrudrButton extends Component {\n\n  render() {\n    const { variant, size, path, icon, children, ...rest } = this.props;\n    const btnClass = `btn btn-${variant}`;\n\n    return (\n      <React.Fragment>\n        {variant === 'link' ?\n          <Link href={path}>\n            <a className={btnClass}>\n              {icon ? <Grudr.components.Icon name={icon} /> : null}\n              {children}\n            </a>\n          </Link>\n        :\n          <Button variant={variant} size={size} {...rest}>\n            {icon ? <Grudr.components.Icon name={icon} /> : null}\n            {children}\n          </Button>\n        }\n      </React.Fragment>\n    )\n  }\n}\n\nGrudr.registerComponent('Button', GrudrButton);\n","/Users/vipin.lahoti/Documents/meteor/Firebase/components/common/Error404.jsx",["141","142","143","144","145","146"],"import Grudr from 'meteor/grudr:lib';\nimport React from 'react';\nimport { FormattedMessage } from 'react-intl';\nimport { Container, Row, Col, Card } from 'react-bootstrap';\n\nconst Error404 = () =>\n  <div className=\"section\">\n    <Container>\n      <Row>\n        <Col sm={12} md={5} lg={5}>\n          <h4 className=\"display-3\"><FormattedMessage id=\"pages.404\"/></h4>\n        </Col>\n\n        <Col sm={12} md={7} lg={7}></Col>\n      </Row>\n    </Container>\n  </div>\n\nGrudr.registerComponent('Error404', Error404);\n","/Users/vipin.lahoti/Documents/meteor/Firebase/components/common/Footer.jsx",["147","148","149","150"],"import Grudr from '@grudr';\nimport React from 'react';\nimport { Container, Row, Col } from 'react-bootstrap';\n\nconst Footer = () =>\n  <footer className=\"section-small\">\n    <Container>\n      <Row>\n        <Col md={7}>\n          <h4 className=\"mb-0 font-weight-light\">Lorem ipsum dolor sit amet, consectetur adipiscing elit.</h4>\n          <hr />\n          <div className=\"copyright\">&copy; 2020.</div>\n        </Col>\n        <Col md={5}>\n\n        </Col>\n      </Row>\n    </Container>\n  </footer>\n\nGrudr.registerComponent('Footer', Footer);\n","/Users/vipin.lahoti/Documents/meteor/Firebase/components/common/HeadTags.jsx",["151"],"import Grudr from '@grudr';\nimport React, { Component } from 'react';\nimport PropTypes from 'prop-types';\nimport Head from 'next/head';\n\nclass HeadTags extends Component {\n  render() {\n\n    const url = !!this.props.url ? this.props.url : 'Grudr';\n    const title = !!this.props.title ? this.props.title : 'Grudr';\n    const description = !!this.props.description ? this.props.description : 'tagline';\n\n    // default image meta: logo url, else site image defined